<!DOCTYPE html>
<html lang="en">
<head>
  <title><%= title %></title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
</head>
<body>

<div class="container">
  <h2><%= title %></h2>
  <a href="/add" class="btn btn-sm btn-primary">Registration Form</a><br/><br/>
  <table class="table table-dark table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Reg No.</th>
        <th>Phone No.</th>
        <th>Vehicle No.</th>
        <th>Services</th>
        <th>Date</th>
        <th>Status</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
        <% users.forEach(function(user){ %>
            <tr>
                <td><%= user.username %></td>
                <td><%= user.reg_no %></td>
                <td><%= user.phone_no %></td>
                <td><%= user.vehicle_no %></td>
                <td><%= user.services%></td>
                <td><%= user.date.toLocaleDateString('en-GB') %></td>
                <td><%= user.status%></td>
                <td>
                    <a href="edit/<%= user.id %>" class="btn btn-sm btn-primary">Edit</a>
                    <a href="delete/<%= user.id %>" class="btn btn-sm btn-danger">Delete</a>
                </td>
            </tr>
        <% }); %>
    </tbody>
  </table>
  <button class="button" onclick="location.href='/'">Back to homepage</button> 
</div>
</body>
</html>